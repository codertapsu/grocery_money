{
  "name": "truffle",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf './build' && rm -rf './types'",
    "compile": "truffle compile",
    "migrate": "truffle migrate",
    "compile:migrate": "tsc -p ./tsconfig.migrate.json --outDir migrations",
    "generate-types": "npx typechain --target=truffle-v5 'build/contracts/*.json' && npx typechain --target=web3-v1 './build/contracts/*.json'",
    "develop": "npm run clean && npm run compile && npm run generate-types && npm run compile:migrate",
    "coverage": "truffle run coverage",
    "typecheck": "tsc --noEmit",
    "ganache": "ganache"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "@truffle/hdwallet-provider": "^2.0.14",
    "@typechain/truffle-v5": "^8.0.1",
    "@typechain/web3-v1": "^6.0.1",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.13",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "bn.js": "^5.2.1",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-standard": "^5.0.0",
    "ganache": "^7.4.1",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "solc": "^0.8.16",
    "solidity-coverage": "^0.7.21",
    "truffle": "^5.5.28",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.2"
  }
}
